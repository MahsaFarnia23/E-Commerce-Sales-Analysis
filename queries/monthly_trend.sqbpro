--Monthly Trend:
SELECT
    order_year,
    order_month,
    order_year || '-' || order_month AS year_month,
    ROUND(SUM(sales), 2)  AS monthly_sales,
    ROUND(SUM(profit), 2) AS monthly_profit
FROM orders_clean
GROUP BY
    order_year,
    order_month
ORDER BY
    order_year,
    order_month;

	

